  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#676767"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#edc1cd" // todo
      }
    },
    {
      "name": "PHP Constants",
      "scope": [
        "constant.other.php"
      ],
      "settings": {
        "foreground": "#e8a1c7"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff" // todo
      }
    },
    {
